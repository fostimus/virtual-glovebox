{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "assets": "./src/assets",
          "screens": "./src/screens",
          "components": "./src/components",
          "base": "./src/components/base",
          "tailwind": "./src/tailwind"
        }
      }
    ]
  ]
}